{
  View v=inflater.inflate(R.layout.fragment_mainmenu,container,false);
  final int[] CLICKABLES=new int[]{R.id.easy_mode_button,R.id.hard_mode_button,R.id.show_achievements_button,R.id.show_leaderboards_button,R.id.sign_in_button,R.id.sign_out_button};
  for (  int i : CLICKABLES) {
    v.findViewById(i).setOnClickListener(this);
  }
  return v;
}
