{
  if (keepPlaying) {
    return;
  }
  this.showGuessResult=showGuessResult;
  this.activity=activity;
  drawView=activity.mDrawView;
  currentPlaybackStrokeIndex=0;
  keepPlaying=true;
  if (strokes.size() == 0) {
    doEndOfReplay();
    return;
  }
  for (  Stroke stroke : strokes) {
    stroke.reset();
  }
  activity.onPlaybackStarted();
  mHandler.post(r);
}
