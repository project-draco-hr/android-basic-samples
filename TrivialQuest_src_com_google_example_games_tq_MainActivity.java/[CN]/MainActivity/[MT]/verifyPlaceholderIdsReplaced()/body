{
  final boolean CHECK_PKGNAME=false;
  if (CHECK_PKGNAME && getPackageName().startsWith("com.google.example."))   return false;
  int res_ids[]=new int[]{R.string.app_id,R.string.trivial_victory_achievement_id};
  for (  int i : res_ids) {
    if (getString(i).equalsIgnoreCase("ReplaceMe"))     return false;
  }
  return true;
}
