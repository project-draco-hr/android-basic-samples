{
  if (item.getItemId() == R.id.menu_sync) {
    loadFromSnapshot();
    return true;
  }
  if (item.getItemId() == R.id.menu_save) {
    saveSnapshot();
    return true;
  }
  if (item.getItemId() == R.id.menu_select) {
    showSnapshots(getString(R.string.title_saved_games),true,true);
  }
  return false;
}
